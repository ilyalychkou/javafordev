<?xml version="1.0" encoding="UTF-8" ?>
<schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNameSpace="http://www.example.com/companies"
    xmlns:tns="http://www.example.com/companies" elementFormDefault="qualified">
<element name="companies">
    <complexType>
        <sequence>
            <element name="company" type="tns:Company" minOccurs="1" maxOccurs="unbounded"/>
        </sequence>
    </complexType>
</element>
    <complexType name="Company">
        <sequence>
            <element name="name" type="string"/>
            <element name="peopleCount" type="integer"/>
        </sequence>
    </complexType>
</schema>